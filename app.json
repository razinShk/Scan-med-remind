{
  "expo": {
    "name": "MedRemind",
    "slug": "medremind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logomed.png",
    "scheme": "medremind",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codecrafters.ScanMedRemind"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logomed.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pinnacle.ScanMedRemind",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logomed.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/NoBgLogoMed.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#4CAF50"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cdb14baa-b073-435f-b904-510945644712"
      }
    }
  }
}
